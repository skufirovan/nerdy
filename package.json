{
  "name": "nerdy",
  "version": "1.0.0",
  "main": "src/main.ts",
  "scripts": {
    "dev": "tsx --watch src/main.ts",
    "build": "npm run clean && npm run babel",
    "clean": "rm -rf dist",
    "babel": "babel src --out-dir dist --extensions .ts,.js --copy-files",
    "start": "node dist/main.js",
    "start:prod": "NODE_ENV=production node dist/main.js",
    "build:tsc": "npm run clean && npm run compile && npm run resolve-paths",
    "compile": "tsc --noEmit false --skipLibCheck",
    "resolve-paths": "tsc-alias",
    "test": "jest"
  },
  "author": "",
  "license": "ISC",
  "description": "Telegram game bot",
  "dependencies": {
    "@prisma/client": "^6.9.0",
    "axios": "^1.10.0",
    "dotenv": "^16.5.0",
    "telegraf": "^4.16.3",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.0",
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-typescript": "^7.27.1",
    "@jest/globals": "^30.0.2",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^24.0.1",
    "babel-plugin-module-resolver": "^5.0.2",
    "jest": "^30.0.2",
    "module-alias": "^2.2.3",
    "nodemon": "^3.1.10",
    "prisma": "^6.9.0",
    "ts-jest": "^29.4.0",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.3",
    "ttypescript": "^1.5.15",
    "typescript": "^5.8.3",
    "typescript-transform-paths": "^3.5.5"
  }
}
